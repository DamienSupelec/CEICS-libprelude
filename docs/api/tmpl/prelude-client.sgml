<!-- ##### SECTION Title ##### -->
prelude-client

<!-- ##### SECTION Short_Description ##### -->


<!-- ##### SECTION Long_Description ##### -->
<para>

<example>
<title>Creating a new analyzer.</title>
<programlisting>
int main(int argc, char **argv)
{
        prelude_client_t *client;

        client = prelude_client_new(PRELUDE_CLIENT_CAPABILITY_SEND_IDMEF);
        if ( ! client )
                return -1;

        ret = prelude_client_init(client, "MyAnalyzerName", "/etc/MyAnalyzerName.conf", argc, argv);
        if ( ret < 0 )
                exit(1);

        exit(0);
}
</programlisting>
</example>

</para>

